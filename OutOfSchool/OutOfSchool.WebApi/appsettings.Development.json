{
  "Logging": {
    "LogLevel": {
      "Default": "Information",
      "Microsoft": "Warning",
      "Microsoft.Hosting.Lifetime": "Information"
    },
    "FilePath": "Loggs\\log.log"
  },

  "Elasticsearch": {
    "EnsureIndex": false,
    "DefaultIndex": "workshop",
    "Urls": [
      "http://localhost:9200/"
    ],
    "User": "admin",
    "Password": "admin"
  },

  "PictureSettings": {
    "MaxSizeBytes": 10485760,
    "TeacherPictureSizeParameter": 512,
    "ProviderAndWorkshopPictureSizeParameters": {
      "Width": 1200,
      "Height": 720
    }
  }
}